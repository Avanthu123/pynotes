   "source": [
    "def factorial(num):\n",
    "    if num < 0:\n",
    "        return \"Enter valid integer.\"\n",
    "    fact = 1\n",
    "    for i in range(1, num+1):\n",
    "          fact *= i\n",
    "    return fact"
   ]
