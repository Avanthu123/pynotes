   "source": [
    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "\n",
    "r = requests.get('https://en.wikipedia.org/wiki/2024_Indian_Premier_League')\n",
    "\n",
    "soup = BeautifulSoup(r.content, 'html.parser')\n",
    "\n",
    "tags = soup.find_all('h2')\n",
    "for i in tags:\n",
    "    print(\"\\nArticle: \", i.text)"
   ]
  }
